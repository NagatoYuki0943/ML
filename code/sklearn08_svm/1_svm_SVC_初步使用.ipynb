   "source": [
    "# 特征值\n",
    "x = [[0, 0], [1, 1]]\n",
    "# 目标值\n",
    "y = [0, 1]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "8799defb",
   "metadata": {},
   "outputs": [
    {
